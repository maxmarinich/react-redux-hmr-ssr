{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {},
  "plugins": [],
  "rules": {
    "max-len": 0,
    "comma-dangle": 0,
    "semi": [
      "error", "never"
    ],
    "indent": [
      2, 2, { "SwitchCase": 1 }
    ],
    "no-debugger": 2,
    "no-console": 1,
    "new-cap": 0,
    "strict": 2,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-extra-boolean-cast": 0,
    "eol-last": 2,
    "quotes": [2, "single"],
    "no-undef": 1,
    "no-unused-vars": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  }
}
